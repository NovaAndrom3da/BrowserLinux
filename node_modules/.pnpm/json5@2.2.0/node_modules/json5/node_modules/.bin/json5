#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/runner/BrowserLinux/macos-web/node_modules/.pnpm/json5@2.2.0/node_modules/json5/lib/node_modules:/home/runner/BrowserLinux/macos-web/node_modules/.pnpm/json5@2.2.0/node_modules/json5/node_modules:/home/runner/BrowserLinux/macos-web/node_modules/.pnpm/json5@2.2.0/node_modules:/home/runner/BrowserLinux/macos-web/node_modules/.pnpm/node_modules:/home/runner/BrowserLinux/macos-web/node_modules:/home/runner/BrowserLinux/node_modules:/home/runner/node_modules:/home/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/runner/BrowserLinux/macos-web/node_modules/.pnpm/json5@2.2.0/node_modules/json5/lib/node_modules:/home/runner/BrowserLinux/macos-web/node_modules/.pnpm/json5@2.2.0/node_modules/json5/node_modules:/home/runner/BrowserLinux/macos-web/node_modules/.pnpm/json5@2.2.0/node_modules:/home/runner/BrowserLinux/macos-web/node_modules/.pnpm/node_modules:/home/runner/BrowserLinux/macos-web/node_modules:/home/runner/BrowserLinux/node_modules:/home/runner/node_modules:/home/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../lib/cli.js" "$@"
else
  exec node  "$basedir/../../lib/cli.js" "$@"
fi
